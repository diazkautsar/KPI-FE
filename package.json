{
  "name": "kpi-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --config ./.prettierrc.json --write \"**/*.tsx\"",
    "prepare": "husky install",
    "precommit": "npx husky add .husky/pre-commit \"npx lint-staged\""
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "husky": "^8.0.3",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
